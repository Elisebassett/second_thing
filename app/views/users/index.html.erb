<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<p>This is <%= @action %></p>

<p>See all the pretty ponies:</p>

<% @user.reverse.each do |user| %>
	<p><%= user.first_name %> <%= user.last_name %>
	<%= link_to "Show #{user.full_name}", user %> <%= link_to "Edit", edit_user_path(user) %>
	<%= link_to "Destroy", user, method: :delete %></p>
<% end %>